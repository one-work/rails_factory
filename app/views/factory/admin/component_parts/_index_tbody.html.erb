<td><%= button_to model.part.name, { controller: 'products', action: 'show', id: model.part_id }, text: true %></td>
<td><%= model.part.name %></td>
<td>
  <%= form_with theme: 'switch', model: model, url: { action: 'update', id: model.id }, data: { controller: 'input' } do |f| %>
    <%= f.check_box :default, data: { action: 'input#submit' }, id: nil %>
  <% end %>
</td>
<td><%= model.min %></td>
<td><%= model.max %></td>
<td>
  <%= button_to({ action: 'destroy', id: model.id }, method: :delete, form: { data: { turbo_confirm: t('.destroy.confirm') } }, class: 'button is-small is-borderless') do %>
    <%= svg_tag 'trash', class: 'icon text-danger' %>
    <span class="ml-1"><%= t('.destroy.title') %></span>
  <% end %>
</td>
